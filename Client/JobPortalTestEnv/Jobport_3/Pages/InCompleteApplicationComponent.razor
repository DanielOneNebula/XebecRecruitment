@page "/incomplete"

<style>

    .incomplete-wrapper {
        width: 100%;
        height: 100vh;
        display: flex;
        flex-direction: column;
        justify-content: center;
        align-items: center;
    }

    .btn-container {
        width: 100%;
        display: flex;
        justify-content: center;
    }

        .btn-container a {
            padding: 8px 15px;
            border-radius: 20px;
            background-color: #54c568;
            text-decoration: none;
            color: #fff;
            transition: 0.5s;
        }

            .btn-container a:hover, .second:hover {
                background-color: #9b66b3;
            }
</style>

<div class="incomplete-wrapper">

    <h3>Sorry, your profile is incomplete. Please complete it before applying</h3>

    <div class="btn-container">

        <a href="/jobboard">Go Back</a>
        <a href="/profile_" class="second ml-2" style="background-color: #57c1bf;">Go To Profile</a>
    </div>
</div>